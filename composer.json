{
  "require": {
    "php": "^7.2",
    "skiphog/http": "^1.0",
    "skiphog/view": "^1.0",
    "skiphog/cache": "^1.0",
    "skiphog/router": "^1.0",
    "skiphog/container": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/System/functions.php"
    ]
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/skiphog/http"
    },
    {
      "type": "git",
      "url": "https://github.com/skiphog/view"
    },
    {
      "type": "git",
      "url": "https://github.com/skiphog/cache"
    },
    {
      "type": "git",
      "url": "https://github.com/skiphog/router"
    },
    {
      "type": "git",
      "url": "https://github.com/skiphog/container"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
